{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Build Debug XcodeProjectCLI (MacOS)",
            "program": "${workspaceFolder}/.build/debug/XcodeProjectCLI",
            "preLaunchTask": "swift: Build Debug XcodeProjectCLI",
            "args": [
                "buildSettings",
                "--workspace-folder",
                "/Users/wang.lun/Work/line-stickers-ios",
                "--xcodeproj",
                "UserStickers/UserStickers.xcodeproj",
                "--target",
                "UserStickers",
                "--configuration",
                "Debug",
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Build Debug sourcekit-bsp",
            "program": "${workspaceFolder}/.build/debug/sourcekit-bsp",
            "preLaunchTask": "swift: Build Debug sourcekit-bsp"
        },
        {
            "type": "lldb",
            "request": "attach",
            "waitFor": true,
            "name": "Wait and attach sourcekit-bsp",
            "program": "${workspaceFolder}/.build/debug/sourcekit-bsp"
        },
        {
            "type": "swift",
            "request": "launch",
            "args": [],
            "cwd": "${workspaceFolder}",
            "name": "Release sourcekit-bsp",
            "program": "${workspaceFolder}/.build/release/sourcekit-bsp",
            "preLaunchTask": "swift: Build Release sourcekit-bsp"
        },
        {
            "type": "swift",
            "request": "launch",
            "args": [],
            "cwd": "${workspaceFolder}",
            "name": "Debug sourcekit-bsp",
            "program": "${workspaceFolder}/.build/debug/sourcekit-bsp",
            "preLaunchTask": "swift: Build Debug sourcekit-bsp"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach to sourcekit-bsp",
            "program": "${workspaceFolder}/.build/debug/sourcekit-bsp",
            "waitFor": true,
            "preLaunchTask": ""
        },
        {
            "type": "swift",
            "request": "launch",
            "args": [],
            "cwd": "${workspaceFolder}",
            "name": "Debug XcodeProjectCLI",
            "program": "${workspaceFolder}/.build/debug/XcodeProjectCLI",
            "preLaunchTask": "swift: Build Debug XcodeProjectCLI"
        },
        {
            "type": "swift",
            "request": "launch",
            "args": [],
            "cwd": "${workspaceFolder}",
            "name": "Release XcodeProjectCLI",
            "program": "${workspaceFolder}/.build/release/XcodeProjectCLI",
            "preLaunchTask": "swift: Build Release XcodeProjectCLI"
        }
    ]
}